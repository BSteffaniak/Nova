package "stabilitytest"

import "nova/standard/thread/Thread"

public class ThreadImplementation extends Thread
{
	int times
	int millis
	
	public construct(int times, int millis)
	{
		this.times  = times
		this.millis = millis
	}
	
	public run()
	{
		for (int i = 0; i < times; i++)
		{
			Thread.sleep(millis)
		}
	}
}