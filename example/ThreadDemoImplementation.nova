import "Thread"

public class ThreadDemoImplementation extends Thread
{
	long	millis
	
	String	word
	
	public ThreadDemoImplementation(long millis, String word)
	{
		this.millis = millis
		this.word   = word
	}
	
	public void run()
	{
		for (int i = 0; i < 10; i++)
		{
			Console.writeLine(word)
			
			sleep(millis)
		}
	}
}