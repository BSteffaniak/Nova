import "IO";

public class BodyBuilder extends Person
{
	visible int weightClass;
	
	public BodyBuilder(int weightClass, String name)
	{
		age = 5;
		
		this.weightClass = weightClass;
		this.name        = name;
	}
	
	public void sayHello()
	{
		IO.println("Hello from " + name + " the BodyBuilder");
	}
}