import "ArrayList";

public class ArrayListDemo
{
	public static void main(String args[])
	{
		char c = 'y';
		
		while (c == 'y' || c == 'Y')
		{
			ArrayList list = new ArrayList();
			
			list.add(1);
			list.add(2);
			list.add(4);
			list.add(3);
			
			for (int i = 0; i < list.size; i++)
			{
				IO.println("Contains: " + list.get(i));
			}
			
			IO.print("Run again? (Y/N)");
			
			c = IO.getChar();
		}
		
		IO.println("\nFinished");
		IO.waitForEnter();
	}
}