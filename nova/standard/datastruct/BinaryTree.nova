package "nova/standard/datastruct"

public class BinaryTree<E extends Comparable> extends Tree<E>
{
	public BinaryNode<E> root
	{
		get
		{
			return super.root
		}
		
		set(BinaryNode node)
		{
			super.root = node
		}
	}
	
	public addNode(Comparable data) -> BinaryTree<E>
	{
		if (root == null)
		{
			root = new BinaryNode(data)
		}
		else
		{
			root.addChild(data)
		}
		
		return this
	}
}