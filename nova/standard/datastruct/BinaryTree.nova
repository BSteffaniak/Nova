package "nova/standard/datastruct"

public class BinaryTree extends Tree
{
	public BinaryNode<Object> root
	{
		get
		{
			return super.root
		}
		
		set(BinaryNode node)
		{
			super.root = node
		}
	}
	
	public addNode(Comparable<BinaryNode<Object>> data)
	{
		if (root == null)
		{
			root = new BinaryNode(data)
		}
		else
		{
			root.addChild(data)
		}
	}
}