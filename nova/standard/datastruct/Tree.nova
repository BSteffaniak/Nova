package "nova/standard/datastruct"

import "nova/standard/exception/UnimplementedOperationException"

public class Tree<E>
{
	public Node<E> root
	
	public preorder() -> Array<E>
	{
		return root.preorder()
	}
	
	public inorder() -> Array<E>
	{
		return root.inorder()
	}
	
	public postorder() -> Array<E>
	{
		return root.postorder()
	}
	
	public levelorder() -> Array<E>
	{
		return root.levelorder()
	}
}