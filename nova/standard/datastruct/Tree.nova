package "nova/standard/datastruct"

import "nova/standard/exception/UnimplementedOperationException"

public class Tree
{
	public Node root
	
	public preorder() -> String
	{
		return root.preorder()
	}
	
	public inorder() -> String
	{
		return root.inorder()
	}
	
	public postorder() -> String
	{
		return root.postorder()
	}
	
	public levelorder() -> String
	{
		throw new UnimplementedOperationException("Level order has not been implemented yet... hold on.")
	}
}