package "nova/standard/math/logic"

import "nova/standard/datastruct/ArrayList"
import "nova/standard/math/Statement"

public class LogicalStatement extends Statement
{
	String statement
	
	ArrayList<StatementComponent> components
	
	public construct(String statement)
	{
		this.statement = statement
		
		components = new ArrayList()
	}
	
	public toString() -> String
	{
		return statement
	}
}