package "nova/standard/primitive/number"

public class Short extends Number
{
	Short value
	
	construct(Short value)
	{
		this.value = value
	}
	
	public static numDigits(Short number) -> Int
	{
		return Long.numDigits(number)
	}
	
	public static toString(Short value) -> String
	{
		return Long.toString(value)
	}
}