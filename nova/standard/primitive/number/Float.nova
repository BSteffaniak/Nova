package "nova/standard/primitive/number"

public class Float extends Number
{
	Float value
	
	construct(Int value)
	{
		this.value = value
	}
	
	public static numDigits(Float number) -> Int
	{
		return Double.numDigits(number)
	}
	
	public static toString(Float value) -> String
	{
		return Double.toString(value)
	}
	
	public toString() -> String
	{
		return toString(value)
	}
}