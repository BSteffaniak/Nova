package "nova/standard/primitive/number"

public class Int extends Number
{
	external int atoi(char str[])
	
	public int value
	
	public construct(int value)
	{
		this.value = value
	}
	
	public static numDigits(int number) -> int
	{
		return Long.numDigits(number)
	}
	
	public static toString(int value) -> String
	{
		return Long.toString(value)
	}
	
	public toString() -> String
	{
		return toString(value)
	}
	
	public parseInt(String num) -> int
	{
		return atoi(num.toCharArray())
	}
}