package "nova/standard/primitive"

public class Bool extends Primitive
{
	Bool value
	
	construct(Bool value)
	{
		this.value = value
	}
	
	public static toString(Bool value) -> String
	{
		if (value)
		{
			return "true"
		}
		
		return "false"
	}
	
	public toString() -> String
	{
		return toString(value)
	}
}