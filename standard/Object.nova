import "NativeObject.h"

public class Object
{
	external char[] hashCode(Object obj)
	
	getHashCode() -> String
	{
		return new String(hashCode(this))
	}
	
	public toString() -> String
	{
		return "[Object @" + getHashCode() + "]"
	}
	
	public equals(Object another) -> bool
	{
		return this == another
	}
}