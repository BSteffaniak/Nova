import "ListNode"

public class List
{
	ListNode	start
	ListNode	current
	
	public getFirst() -> ListNode
	{
		return start
	}
	
	public add(Object data)
	{
		ListNode node = new ListNode(data)
		
		if (start == null)
		{
			start   = node
			current = node
		}
		else
		{
			current.setNext(node)
		}
		
		current = node
	}
	
	public remove(Object data)
	{
		if (start.getData() == data)
		{
			start = start.getNext();
		}
		
		ListNode prev = start
		ListNode cur  = start.getNext()
		
		while (cur != null)
		{
			Object d = cur.getData()
			
			if (d == data)
			{
				prev.setNext(cur.getNext())
			}
			
			cur = cur.getNext()
		}
	}
}